<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android = "http://schemas.android.com/apk/res/android"
    package = "com.yujongu.socialserviceagent">

    <uses-permission android:name = "android.permission.INTERNET" />

    <application
        android:name = ".KakaoLogin.GlobalApplication"
        android:allowBackup = "true"
        android:icon = "@drawable/ssasquareicon"
        android:label = "@string/app_name"
        android:roundIcon = "@drawable/ssacircleicon"
        android:supportsRtl = "true"
        android:theme = "@style/AppTheme">
        <activity android:name = ".AddFriendsActivity"></activity>
        <activity android:name = ".EditProfileActivity" />
        <activity android:name = ".DayActivity" />
        <activity android:name = ".ProfileActivity" />
        <activity
            android:name = ".SplashActivity"
            android:theme = "@style/SplashTheme">
            <intent-filter>
                <action android:name = "android.intent.action.MAIN" />

                <category android:name = "android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name = ".KakaoLogin.LoginActivity" />
        <activity android:name = ".MainActivity" />

        <meta-data
            android:name = "com.kakao.sdk.AppKey"
            android:value = "@string/kakao_app_key" />
    </application>

</manifest>